<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.excel.dao.AngentDao">
    <insert id="addAngent" parameterType="com.example.excel.bean.Angent">
        insert into angent values(a_id,#{a_name},#{rid},#{a_loginid},#{a_pwd},#{a_record},#{a_phone},#{a_idcard},#{a_bankid},#{a_from},#{a_inttime},#{pf_id},#{a_type})
    </insert>
</mapper>